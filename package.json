{
  "name": "electron-umi",
  "description": "`@umijs/max` 模板项目，更多功能参考 [Umi Max 简介](https://umijs.org/docs/max/introduce)",
  "version": "0.0.17-5",
  "homepage": ".",
  "main": "main.js",
  "build": {
    "appId": "ElectronUmi",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "compression": "store",
    "asar": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/if0r/electron-umi.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/if0r/electron-umi/issues"
  },
  "private": true,
  "author": "ifor <ifor@cloras.io>",
  "scripts": {
    "ebm": "yarn build && electron-builder --win --x64",
    "eb": "yarn build && electron .",
    "dev": "max dev",
    "build": "max build",
    "format": "prettier --cache --write .",
    "prepare": "husky install",
    "postinstall": "max setup",
    "release": "standard-version",
    "setup": "max setup",
    "start": "npm run dev"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.4.4",
    "@umijs/max": "^4.0.68",
    "antd": "^5.4.0",
    "electron-updater": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "electron": "^24.3.0",
    "electron-builder": "^23.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.0.3"
  }
}
